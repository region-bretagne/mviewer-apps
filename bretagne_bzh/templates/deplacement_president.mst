	text-decoration:none;
	font-family:'OpenSans-Bold',sans-serif;
	font-weight:300;
	color:#FFFFFF;
	background-color:#B0006B;
	text-align:center;
	font-size:small;
}
.rb-but-jaune{
	display:inline-block;
	margin:0.5em 0.3em 0.3em 0.5em;
	border-radius:0.3em;
