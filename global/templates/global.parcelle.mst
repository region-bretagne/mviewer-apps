{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h3 class="rb-title-feature">{{nationalcadastralreference}}</h3>

		<p class="rb-text-feature">		
			<span style="font-weight:bold">Numéro :</span> {{label}}<br/>
			<span style="font-weight:bold">Département :</span> {{departement}}<br/>
			<span style="font-weight:bold">Section :</span> {{geo_section}}<br/>
			<span style="font-weight:bold">Surface :</span> {{areavalue}} m²<br/>
       </p>
	</li>
{{/features}}