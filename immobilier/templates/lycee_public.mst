{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h3 class="rb-title-feature">{{lib_etablissement}}</h3>
		<p class="rb-text-feature">		
			<b>Code : {{code_uai}}</b><br><br>
			<b>Ville :</b> {{ville}}<br>
			<b>Type etablissement :</b><br> {{type_etablissement}}<br>
			<b>Reseau etablissement :</b><br> {{reseau_etablissement}}<br>
			<b>Année ouverture :</b> {{annee_ouverture}}<br>
			<b>Surface cadastrée :</b> {{surface_cadastree}} m²<br>
			<b>Surface de plancher :</b> {{surface_totale}} m²<br>
			<b>Classement radon :</b> {{classement_radon}}<br>
			<b>EMAT :</b> {{emat}}<br>
			<b>Numéro comptable :</b> {{numero_comptable}}<br>
			<b>Numéro SIRET :</b> {{siret}}<br>
			<b>Centralisation téléphonique :</b> {{cent_tel}}<br>
			<b>Centralisation informatique :</b> {{cent_inf}}<br>
			<b>Photovoltaïque :</b> {{photovoltaique}}<br>
		</p>
		{{#lien_dossier}}
		<div class="rb-but-list">
			<a href="{{lien_dossier}}" target="_blank" title="Site web" class="rb-but-link"><span class="glyphicon glyphicon-file" aria-hidden="true"></span> <b>Dossier d'inventaire</b></a>
		</div>
		{{/lien_dossier}}

	</li>
{{/features}}

<style>
/* test icon rb*/
.icon-information{
	font-size : 20px;
	color : #FFFFFF;
}
</style>
