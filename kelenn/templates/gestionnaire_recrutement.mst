{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h2 class="rb-title-feature">{{nom}} - {{ville}}</h2>
		
		<h3 class="rb-subtitle-feature">Gestionnaire</h3>
		<p class="rb-text-feature">
			<img src="https://kartenn.region-bretagne.fr/kartoviz/apps/region/kelenn/img/citoyen.svg" width=25 height=25>{{gestionnaire_recrutement}}<br/>
			<img src="https://kartenn.region-bretagne.fr/kartoviz/apps/region/kelenn/img/telephone.svg" width=25 height=25> {{telephone_gestionnaire_recrutement}}<br/>
			<img src="https://kartenn.region-bretagne.fr/kartoviz/apps/region/kelenn/img/enveloppe.svg" width=25 height=25> {{mail_gestionnaire_recrutement}}<br/>
		</p>
		
		<h3 class="rb-subtitle-feature">Assistant.e recrutement</h3>
