        {{#photo_1}}
			<img src="{{photo_1}}" class="img-responsive center-block" />
        {{/photo_1}}
		<p class="rb-text-feature">
			<span style="font-weight:bold"> Commune : </span>{{commune}}<br/>
			{{#localisation}}
