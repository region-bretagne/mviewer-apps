{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h3 class="rb-title-feature">{{nom}}</h3>  
        
        <p class="rb-text-feature">
			<span style="font-weight:bold"> Equipement :</span> {{code_equipement}}<br/>
			<span style="font-weight:bold"> Niveau :</span> {{niveau}}<br/>
			<span style="font-weight:bold"> Type équipement :</span> {{type_equipement}}<br/>
			<span style="font-weight:bold"> Date mise en service :</span> {{date_service}}<br/>
			<span style="font-weight:bold"> Etat équipement :</span> {{etat_equipement}}<br/>
			<span style="font-weight:bold"> Equipement :</span> {{code_equipement}}<br/>

            {{#langues}}                
                <span style="font-weight:bold"> Langues :</span> {{langues}}<br/>
            {{/langues}}
            {{#commentaire}}                
                <span style="font-weight:bold"> Commentaire :</span> {{commentaire}}<br/>
            {{/commentaire}}

        </p>
      
        {{#url_photo}}
            <img src="{{url_photo}}" class="img-responsive" data-title="{{nom}}" />
        {{/url_photo}}       

    
		
		
	    
       
	</li>
{{/features}}

