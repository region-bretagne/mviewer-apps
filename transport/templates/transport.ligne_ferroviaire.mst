{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h3 class="rb-title-feature">{{nom_ligne}}</h3>
		<p class="rb-text-feature">
			<span style="font-weight:bold"> Axe :</span> {{axe}}<br/>
			<span style="font-weight:bold">Marché :</span> {{marche}}<br/>
			<span style="font-weight:bold">Marché detaillé :</span> {{marche_detail}}<br/><br/>
			<span style="font-weight:bold">Type :</span> {{typologie}}<br/>
		</p>
	</li>
{{/features}}