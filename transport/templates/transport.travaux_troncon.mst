{{#features}}
	<li class="item" id="{{feature_ol_uid}}">
		<h3 class="rb-title-feature">Travaux entre {{gare_troncon_1}} et {{gare_troncon_2}}</h3>
		<p class="rb-text-feature">		
			<span style="font-weight:bold"> Année :</span> {{annee}}<br/>
			<span style="font-weight:bold"> Type de travaux :</span> {{type_tr}}<br/>
		</p>
		<h4 class="rb-subtitle-feature">Information détaillée</h4>
		<p class="rb-text-feature">		
			{{info_detail}}
		</p>
	</li>
{{/features}}