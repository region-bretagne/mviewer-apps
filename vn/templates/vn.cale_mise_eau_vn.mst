        {{#photo}}                
			<img src="{{photo}}" class="img-responsive" />
        {{/photo}}                
		<p class="rb-text-feature">	
			{{#proprietaire}}     			
				<span style="font-weight:bold"> Propriétaire :</span> {{proprietaire}}<br/>
			{{/proprietaire}}                
			{{#gestionnaire}}     			
